    <script>
        function performSearch() {
            var searchTerm = document.getElementById("search-input").value;
            var searchUrl = "https://temp-number.com/temporary-numbers/Netherlands/31970105395" + searchTerm;
            window.open(searchUrl, "_blank");
        }
    </script>

